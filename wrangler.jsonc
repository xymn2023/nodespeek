{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nodespeek",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": [
		"nodejs_compat"
	],
  "main": "./src/index.tsx",
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nodeseeker",
      "database_id": "c3a2e4dc-eb91-4659-9c79-7af841f89306"
    }
  ],
  "triggers": {
    "crons": [
      "*/1 * * * *"
    ]
  },
  "observability": {
    "enabled": false
  },
	"assets": {
		"directory": "./dist",
		"not_found_handling": "single-page-application"
	},
}
